{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "prescreener" : ["./styles/prescreener.scss"]
  },
  "publicPath": "/app/themes/be-challenge-theme",
  "devUrl": "https://beboilerplate.test",
  "proxyUrl": "https://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/**/*.php",
    "resources/assets/**/*.{scss,css,js,jsx,jpeg,jpg,png,svg,webp,woff,woff2,ttf,otf,mov,ico,mp4,webm}",
    "resources/assets/scripts/vue/**/*.{js,vue}"
  ]
}
